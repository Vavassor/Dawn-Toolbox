{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch index.html",
      "type": "firefox",
      "request": "launch",
      "reAttach": true,
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}/Source",
      "pathMappings": [
        {
          "url": "webpack:///Source",
          "path": "${workspaceFolder}/Source"
        }
      ]
    }
  ]
}
